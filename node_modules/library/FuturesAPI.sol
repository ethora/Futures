pragma solidity ^0.4.17;

contract FuturesAPI {
    
    uint public last;
    uint public high;
    uint public low;
    uint public settlement;
    uint public tick_size;
    uint public tick_value;
    uint public size;
    address public addressTicker;
    uint64 public created;
    uint64 public expire;
    string public name;
    string public symbol;
    uint8 public decimals;

    function setLast(uint _value) public returns (bool);

    function setHigh(uint _value) public returns (bool);

    function setLow(uint _value) public returns (bool);
    
    function setSettlement(uint _value) public returns (bool);

    function transfer(address _to, uint256 _value) public returns (bool);

    function transferFrom(address _from, address _to, uint256 _value) public returns (bool) ;
}